{
  "printWidth": 100,
  "trailingComma": "none",
  "singleQuote": true,
  "tabWidth": 2,
  "semi": false,
  "useTabs": false,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": ["^next", "^react", "<THIRD_PARTY_MODULES>", "", "^@/(.*)$", "^[./]"]
}
